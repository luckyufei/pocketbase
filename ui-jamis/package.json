{
  "name": "cjapi-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "vite dev --host 127.0.0.1",
    "build:vue": "vue-tsc --noEmit",
    "build:analyze": "vite build --mode analyze",
    "build": "cross-env COS_ENV=dev vite build -- --cospath /debug-pbui-jamis",
    "build:dev": "cross-env COS_ENV=dev vite build -- --cospath /dev-pbui-jamis",
    "build:test": "cross-env COS_ENV=test vite build -- --cospath /test-pbui-jamis",
    "build:prod": "cross-env COS_ENV=prod vite build -- --cospath /prod-pbui-jamis",
    "lint": "eslint src/**/*.ts --quiet --no-fix --format stylish",
    "release:dev": "COS_ENV=dev node scripts/upload2cos.mjs --cospath /dev-pbui-jamis  --useHttps",
    "release:test": "COS_ENV=test node scripts/upload2cos.mjs --cospath /test-pbui-jamis  --useHttps",
    "release:prod": "COS_ENV=prod node scripts/upload2cos.mjs --cospath /prod-pbui-jamis  --useHttps",
    "make-check-login": "node scripts/make-check-login.js",
    "tscheck": "npx tsc -w --noEmit",
    "tsc:check-login": "npx tsc ./src/utils/check-login.ts --outDir ./public --removeComments --target es5",
    "tsc:trace": "rm -fr tsconfig.tsbuildinfo && npx tsc -p tsconfig.json --noEmit --generateTrace trace/",
    "analyze-trace": "npx analyze-trace ./trace"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "jamis": "9.0.0-beta.59",
    "js-cookie": "^3.0.1",
    "lodash-es": "4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.10.3",
    "tslib": "^2.3.1",
    "pocketbase": "0.21.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@tencent/eslint-config-tencent": "1.0.0-beta.5",
    "@tencent/jarvis-webpack-cos": "1.1.0",
    "@tencent/jmonitor-web-sdk": "2.0.14",
    "@types/axios": "^0.14.0",
    "@types/history": "4.7.11",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^16.0.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@typescript/analyze-trace": "^0.9.0",
    "@vitejs/plugin-react": "2.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.20.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "glob": "^8.0.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "pnpm": "^7.13.4",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.26.5",
    "typescript": "5.5.4",
    "vite": "3.1.0",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-plugin-vue2": "^2.0.1",
    "vue-template-compiler": "2.6.11",
    "zx": "^7.1.1"
  },
  "packageManager": "pnpm@7.20.0",
  "pnpm": {
    "overrides": {
      "typescript": "5.5.4",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@types/react": "18.0.18",
      "@types/react-dom": "18.0.6",
      "moment": "2.29.4",
      "esbuild": "0.15.7",
      "lodash-es": "4.17.21",
      "@types/lodash-es": "4.17.6",
      "react-json-view": "1.21.3",
      "rc-input-number": "7.3.4",
      "vue": "2.6.11",
      "vue-template-compiler": "2.6.11",
      "react-router": "5.2.1",
      "react-router-dom": "5.3.0",
      "@types/react-router-dom": "5.3.3",
      "@types/react-router": "5.1.18"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        1,
        "always",
        72
      ]
    }
  }
}
